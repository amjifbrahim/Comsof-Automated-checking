{
  
"functions": {
  "Backend/api/validate.py": {
    "runtime": "vercel-python@3.9"
  },
  "Backend/api/export-pdf.py": {
    "runtime": "vercel-python@3.9"
  },
  "Backend/api/health.py": {
    "runtime": "vercel-python@3.9"
  }
},
  "routes": [
    {
      "src": "/api/validate",
      "dest": "Backend/api/validate.py"
    },
    {
      "src": "/api/export-pdf",
      "dest": "Backend/api/export-pdf.py"
    },
    {
      "src": "/api/health",
      "dest": "Backend/api/health.py"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))$",
      "dest": "frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/build/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "./Backend"
  }
}
