{
  "functions": {
    "Backend/api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/api/$1.py"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg)$)",
      "dest": "/Backend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Backend/build/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "./"
  }
}